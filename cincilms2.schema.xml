<database name="CinciLMS">
   <table name="AssignmentFileSubmissions">
      <column name="SubmissionID" type="int" nullable="False"/>
      <column name="AssignmentID" type="int" nullable="False"/>
      <column name="StudentID" type="int" nullable="False"/>
      <column name="CourseID" type="int" nullable="False"/>
      <column name="SubmissionDate" type="date" nullable="False"/>
      <column name="FileName" type="varchar" nullable="False"/>
   
      <index name="PRIMARY" unique="True">
         <column name="SubmissionID"/>
      </index>
      <index name="IDX_StudentAssignmentSubmissions" unique="False">
         <column name="AssignmentID"/>
         <column name="StudentID"/>
         <column name="CourseID"/>
      </index>
      <index name="FK_SubmissionStudentID" unique="False">
         <column name="StudentID"/>
      </index>
   </table>

   <table name="AssignmentTypes">
      <column name="AssignmentTypeID" type="int" nullable="False"/>
      <column name="TypeName" type="varchar" nullable="True"/>
   
      <index name="PRIMARY" unique="True">
         <column name="AssignmentTypeID"/>
      </index>
   </table>

   <table name="Assignments">
      <column name="AssignmentID" type="int" nullable="False"/>
      <column name="TypeID" type="int" nullable="False"/>
      <column name="PointsPossible" type="int" nullable="True"/>
      <column name="DueDate" type="date" nullable="True"/>
   
      <index name="PRIMARY" unique="True">
         <column name="AssignmentID"/>
      </index>
      <index name="FK_AssignmentTypeID" unique="False">
         <column name="TypeID"/>
      </index>
   </table>

   <table name="ContentItemAttachments">
      <column name="ContentID" type="int" nullable="False"/>
      <column name="FileID" type="int" nullable="False"/>
   
      <index name="PRIMARY" unique="True">
         <column name="ContentID"/>
         <column name="FileID"/>
      </index>
      <index name="FK_AttachmentFileID" unique="False">
         <column name="FileID"/>
      </index>
   </table>

   <table name="ContentItems">
      <column name="ItemID" type="int" nullable="False"/>
      <column name="Title" type="varchar" nullable="False"/>
      <column name="Text" type="text" nullable="False"/>
   
      <index name="PRIMARY" unique="True">
         <column name="ItemID"/>
      </index>
   </table>

   <table name="ContentLinks">
      <column name="LinkID" type="int" nullable="False"/>
      <column name="DestinationID" type="int" nullable="False"/>
   
      <index name="PRIMARY" unique="True">
         <column name="LinkID"/>
      </index>
      <index name="FK_LinkDestinationID" unique="False">
         <column name="DestinationID"/>
      </index>
   </table>

   <table name="ContentTypes">
      <column name="ContentTypeID" type="int" nullable="False"/>
      <column name="TypeName" type="varchar" nullable="True"/>
      <column name="DefaultAccess" type="set" nullable="False"/>
   
      <index name="PRIMARY" unique="True">
         <column name="ContentTypeID"/>
      </index>
   </table>

   <table name="CourseContent">
      <column name="ContentID" type="int" nullable="False"/>
      <column name="ParentID" type="int" nullable="True"/>
      <column name="OwnerID" type="int" nullable="False"/>
      <column name="TypeID" type="int" nullable="False"/>
      <column name="Name" type="varchar" nullable="True"/>
      <column name="SortOrder" type="int" nullable="True"/>
      <column name="AccessFlags" type="set" nullable="False"/>
      <column name="CreationTime" type="timestamp" nullable="False"/>
   
      <index name="PRIMARY" unique="True">
         <column name="ContentID"/>
      </index>
      <index name="FK_ContentParentID" unique="False">
         <column name="ParentID"/>
      </index>
      <index name="FK_ContentOwnerID" unique="False">
         <column name="OwnerID"/>
      </index>
      <index name="FK_ContentTypeID" unique="False">
         <column name="TypeID"/>
      </index>
   </table>

   <table name="CourseRoles">
      <column name="RoleID" type="int" nullable="False"/>
      <column name="RoleName" type="varchar" nullable="True"/>
      <column name="DefaultAccess" type="set" nullable="False"/>
   
      <index name="PRIMARY" unique="True">
         <column name="RoleID"/>
      </index>
   </table>

   <table name="Courses">
      <column name="CourseID" type="int" nullable="False"/>
      <column name="CourseCode" type="varchar" nullable="False"/>
      <column name="CourseName" type="varchar" nullable="True"/>
      <column name="EntryPointID" type="int" nullable="True"/>
      <column name="AccessFlags" type="set" nullable="False"/>
   
      <index name="PRIMARY" unique="True">
         <column name="CourseID"/>
      </index>
      <index name="CourseCode" unique="True">
         <column name="CourseCode"/>
      </index>
      <index name="FK_CourseEntryPointID" unique="False">
         <column name="EntryPointID"/>
      </index>
   </table>

   <table name="FactCourseEnrollment">
      <column name="UserID" type="int" nullable="False"/>
      <column name="CourseID" type="int" nullable="False"/>
      <column name="RoleID" type="int" nullable="False"/>
      <column name="AccessFlags" type="set" nullable="False"/>
   
      <index name="PRIMARY" unique="True">
         <column name="UserID"/>
         <column name="CourseID"/>
      </index>
      <index name="FK_EnrollmentCourseID" unique="False">
         <column name="CourseID"/>
      </index>
      <index name="FK_EnrollmentRoleID" unique="False">
         <column name="RoleID"/>
      </index>
   </table>

   <table name="FactFolderContents">
      <column name="FolderID" type="int" nullable="False"/>
      <column name="ContentID" type="int" nullable="False"/>
      <column name="Path" type="varchar" nullable="False"/>
   
      <index name="PRIMARY" unique="True">
         <column name="FolderID"/>
         <column name="ContentID"/>
      </index>
      <index name="IDX_PathIndex" unique="True">
         <column name="FolderID"/>
         <column name="Path"/>
      </index>
      <index name="FolderID" unique="False">
         <column name="FolderID"/>
      </index>
      <index name="ContentID" unique="False">
         <column name="ContentID"/>
      </index>
   </table>

   <table name="FileManagement">
      <column name="FileID" type="int" nullable="False"/>
      <column name="OwnerID" type="int" nullable="False"/>
      <column name="StorageFileName" type="varchar" nullable="True"/>
      <column name="OriginalFileName" type="varchar" nullable="True"/>
   
      <index name="PRIMARY" unique="True">
         <column name="FileID"/>
      </index>
   </table>

   <table name="GradeColumns">
      <column name="ColumnID" type="int" nullable="False"/>
      <column name="CourseID" type="int" nullable="False"/>
      <column name="Name" type="varchar" nullable="True"/>
      <column name="PointsPossible" type="int" nullable="True"/>
      <column name="AssignmentID" type="int" nullable="True"/>
      <column name="SortOrder" type="int" nullable="True"/>
   
      <index name="PRIMARY" unique="True">
         <column name="ColumnID"/>
      </index>
      <index name="FK_ColumnCourseID" unique="False">
         <column name="CourseID"/>
      </index>
      <index name="FK_ColumnAssignmentID" unique="False">
         <column name="AssignmentID"/>
      </index>
   </table>

   <table name="Grades">
      <column name="ColumnID" type="int" nullable="False"/>
      <column name="StudentID" type="int" nullable="False"/>
      <column name="Grade" type="decimal" nullable="True"/>
   
      <index name="PRIMARY" unique="True">
         <column name="ColumnID"/>
         <column name="StudentID"/>
      </index>
      <index name="FK_GradeStudentID" unique="False">
         <column name="StudentID"/>
      </index>
   </table>

   <table name="Users">
      <column name="UserID" type="int" nullable="False"/>
      <column name="ExternalID" type="int" nullable="True"/>
      <column name="Username" type="varchar" nullable="True"/>
      <column name="FirstName" type="varchar" nullable="True"/>
      <column name="MiddleInitial" type="char" nullable="True"/>
      <column name="LastName" type="varchar" nullable="True"/>
      <column name="EmailAddress" type="varchar" nullable="True"/>
      <column name="PasswordSalt" type="char" nullable="True"/>
      <column name="PasswordHash" type="char" nullable="True"/>
      <column name="Notes" type="text" nullable="True"/>
      <column name="LastLogin" type="date" nullable="True"/>
      <column name="IsActive" type="tinyint" nullable="False"/>
      <column name="SystemRole" type="int" nullable="False"/>
   
      <index name="PRIMARY" unique="True">
         <column name="UserID"/>
      </index>
      <index name="Username" unique="True">
         <column name="Username"/>
      </index>
      <index name="ExternalID" unique="False">
         <column name="ExternalID"/>
      </index>
   </table>

</database>

